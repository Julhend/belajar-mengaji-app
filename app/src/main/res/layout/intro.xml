<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#ffffff">

    <ImageView
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:id="@+id/stti"
        android:layout_centerVertical="true"
        android:layout_marginBottom="10dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/stti" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="S1 TEKNIK INFORMATIKA"
        android:textColor="#000000"
        android:id="@+id/jtk"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_below="@+id/stti"  />

    <ProgressBar
        android:id="@+id/progressbar_Horizontal1"
        style="?android:attr/progressBarStyleHorizontal"
        android:progressDrawable="@drawable/custom_progressbar"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:max="96"
        android:layout_above="@+id/stti"
        android:layout_alignLeft="@+id/jtk"
        android:layout_alignStart="@+id/jtk"
        android:layout_toLeftOf="@+id/jtk"
        android:layout_toStartOf="@+id/jtk" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="SEKOLAH TINGGI TEKNOLOGI INDONESIA"
        android:id="@+id/jtm"
        android:layout_below="@+id/jtk"
        android:textColor="#000000"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="TANJUNG PINANG"
        android:textColor="#000000"
        android:id="@+id/tnj"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_below="@+id/jtm"  />
</RelativeLayout>